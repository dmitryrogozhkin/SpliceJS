<sjs-template type="UserApplications.BasicApplication">
	<div>	
		<div><a data-sjs-element="mainMenuButton">Main Menu</a></div>
		<sjs-include>{
			type:'UserControls.Clock', 
			ref:'clock'
		}</sjs-include>
	</div>
</sjs-template>


<sjs-template type="UserControls.Clock">
	<div class="-center" data-sjs-element="mainContainer">
		<span class="-clock-label" data-sjs-element="clockLabel"></span>
		<span style="font-family:Arial;"> UTC -5</span>
	</div>
</sjs-template>


