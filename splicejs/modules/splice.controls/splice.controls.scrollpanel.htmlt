
<sjs-template type="SpliceJS.Controls.ScrollPanel">
	<div class="-splicejs-scrollpanel">
		<div class="clipping" data-sjs-element="clippingArea">
			<div class="client" data-sjs-element="scrollClient"><div>@scrollContent</div></div>
			<div class="static" data-sjs-element="staticContainer"><div>@staticContent</div></div>
		</div>
		<div class="vertical-bar" data-sjs-element="verticalBar"></div>
		<div class="horizontal-bar" data-sjs-element="horizontalBar"></div>
	</div>
</sjs-template>
